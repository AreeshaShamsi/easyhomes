{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "about.html",
        "use": "@vercel/static"
      },
      {
        "src": "contact.html",
        "use": "@vercel/static"
      },
      {
        "src": "projects.html",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  